<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pe.nullpoint.contactme" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme">
        <activity
            android:name=".ContactMe"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name=".view.Activity_register"
            android:label="@string/title_activity_activity_registro" >
        </activity>
        <activity
            android:name=".view.Activity_login"
            android:label="@string/title_activity_activity_login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.Activity_choose"
            android:label="@string/title_activity_activity_choose" >
        </activity>
        <activity
            android:name=".view.Activity_map"
            android:label="@string/title_activity_activity_map" >
        </activity>
        <activity
            android:name=".view.Activity_profile"
            android:label="@string/title_activity_activity_profile" >
        </activity>
        <activity
            android:name=".view.Activity_contact"
            android:label="@string/title_activity_activity_contact" >
        </activity>
        <activity
            android:name=".view.Activity_rating"
            android:label="@string/title_activity_activity_rating" >
        </activity>
        <activity
            android:name=".view.Activity_service"
            android:label="@string/title_activity_activity_servicio" >
        </activity>
    </application>

</manifest>
