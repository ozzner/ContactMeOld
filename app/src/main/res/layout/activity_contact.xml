<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="pe.nullpoint.contactme.view.Activity_contact">

    <TextView android:text="@string/temp_name6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/tv_contact"
        android:textSize="25sp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_contact"
        android:id="@+id/btn_contact"
        android:onClick="contact_click"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="74dp"
        android:layout_alignRight="@+id/tv_contact"
        android:layout_alignEnd="@+id/tv_contact"
        android:layout_alignLeft="@+id/tv_contact"
        android:layout_alignStart="@+id/tv_contact" />

</RelativeLayout>
