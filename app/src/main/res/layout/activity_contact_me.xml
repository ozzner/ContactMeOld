<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".ContactMe">

    <TextView android:text="@string/temp_name9" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/tv_splash"
        android:textSize="25sp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_splash"
        android:id="@+id/btn_splash"
        android:onClick="iniciar_click"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/tv_splash"
        android:layout_alignEnd="@+id/tv_splash"
        android:layout_marginBottom="79dp"
        android:layout_alignLeft="@+id/tv_splash"
        android:layout_alignStart="@+id/tv_splash" />

</RelativeLayout>
